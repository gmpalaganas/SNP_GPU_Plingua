@model<spiking_psystems>

def main(){

    call init_struct();
    call init_rules();

}

def init_struct(){
    
    @mu = 0,1;
    @mu += s{0}, s{1};
    @mu += out;

    @ms(0) = a*5;
    @ms(1) = a*10;

    @marcs = ({i},s{j}): 0 <= i <= 1, 0 <= j <= 1;
    @marcs += (s{i},out): 0 <= i <= 1;

}

def init_rules(){
    [ a --> a ]'{i} "a+": 0 <= i <= 1;
    [ a*2 --> a ]'s{0};
    [ a --> # ]'s{0};
    [ a*2 --> # ]'s{1};
    [ a --> a ]'s{1};
}
